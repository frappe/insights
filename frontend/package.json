		"autoprefixer": "^10.4.7",
		"cypress": "^10.3.0",
		"eslint": "^8.19.0",
		"eslint-plugin-vue": "^9.1.1",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-tailwindcss": "^0.1.11",
		"sass": "^1.54.9",
		"tailwindcss": "^3.1.2",
		"vite": "^2.9.12",
		"vite-plugin-istanbul": "^2.8.0"
	}
